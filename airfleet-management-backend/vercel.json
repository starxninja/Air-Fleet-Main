{
    "installCommand": "npm install --legacy-peer-deps",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ,
        {
            "src": "src/**/*",
            "use": "@vercel/node"
        }
    ],
"routes": [
    {
        "src": "/(.*)",
        "dest": "/"
    }
]
}